{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"],
    },
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "@types/bun",
      "@types/node",
    ],
    "lib": [
      "ESNext",
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
  },
}